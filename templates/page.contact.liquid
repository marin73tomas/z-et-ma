  #over_map { border-radius: 4px 4px 0px 0px;   box-shadow: rgb(0 0 0 / 30%) 3px 1px 70px; position: absolute; top: 150px; left: 50px; z-index: 99; background-color: white; width: 400px; height: 550px}
  #over_map > div > input { width: calc(100%); float: left; border-radius: 0px !important;}
  .cross { width: 45px; height: 45px; border: 1px solid rgba(80, 80, 80, 0.3); padding-top: 10px; cursor: pointer; display: none;}
  @media only screen and (max-width: 768px) {
    #map { width: calc(100vw);}
    #over_map {top: 745px; width: calc(100vw); left: 0px; box-shadow: unset; height: auto; min-height: 50px;}
    #wrapper {overflow: visible; margin-bottom: 70px;}
