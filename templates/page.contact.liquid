<script>
  function switchl(letter) {
    doc = document.querySelector(".result");
    res = []
    for (var i = 0; i < ref.length; i++)  {
      if (ref[i]["Nom du PDV"][0].toLowerCase() == letter){
        res.push(ref[i]);
      }
    }
    res.sort((a, b) => a["Nom du PDV"].localeCompare(b["Nom du PDV"]))
    ret = "";
    for (var i = 0; i < res.length; i++)  {
      ret += '<div style="font-size: 14px; margin-top: 10px; margin-bottom: 10px; border-top: 1px solid rgba(80, 80, 80, 0.3); padding-top: 5px;\">'
         + '<h3 style="margin-bottom: 5px">' + res[i]["Nom du PDV"] + '</br>'
      	 + res[i]["Adresse"] + ', '
         + res[i]["Code postal"] + ', ' + res[i]["Ville"] + '</br>'
      + {% if request.locale.iso_code == 'en' %} "Phone: "{% else %}'Téléphone: '{% endif %} 
         + ((res[i]["Téléphone"] != void 0) ? res[i]["Téléphone"]: '' ) + '</br> </div>'
    }
    if (ret == "") {
      ret = "<h3 style='color: rgba(120, 120, 120, 1); font-size: 20px; margin-top: 30px'>  {% if request.locale.iso_code == 'en' %} No result {% else %} Aucun resultat{% endif %}  </h3>"
    } else {
      ret += '<div style="font-size: 14px; margin-top: 10px; margin-bottom: 10px; border-top: 1px solid rgba(80, 80, 80, 0.3); padding-top: 5px;\"></div>'
    }
    doc.innerHTML = ret;
    }
  
</script>
