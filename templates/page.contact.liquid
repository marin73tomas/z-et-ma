function nearby_shops(marker) {
  var pos = {
    lat: marker.position.lat(),
    lng: marker.position.lng(),
  }
  analyse(pos);
}
  
  function geo(){
    let i = 0;
    let r = void 0;
    for (; i < ref.length; i++){
      r = httpGet(ref[i]["Adresse"] + ", " + ref[i]["Ville"]);
      ref[i]["Geo"] = r;
    }
    console.log(JSON.stringify(ref));
  }
  
function httpGet(address)
{
  	address = encodeURI(address);
    var key = "AIzaSyA7U9otsAEc6fQE0Sfy9kYLrywEtS47bbU"
    var url = "https://maps.googleapis.com/maps/api/geocode/json?address=" + address + "&key=" + key
    var xmlHttp = new XMLHttpRequest();
    xmlHttp.open( "GET", url, false ); // false for synchronous request
    xmlHttp.send( null );
    var geo = JSON.parse(xmlHttp.responseText)["results"][0]["geometry"]["location"];
  return geo;
    
}
  
  function analyse(geo) {
    res = []
    for (var i = 0; i < ref.length; i++){
      a = geo["lat"] - ref[i]["Geo"]["lat"];
      b = geo["lng"] - ref[i]["Geo"]["lng"];
      res[i] = ref[i];
      res[i]["dist"] = (Math.sqrt((a * a) + (b * b)));
    }
    
    res.sort(function (a, b) {
      return a["dist"] - b["dist"];
    });
    doc = document.querySelector("#results");
    ret = "";
    for (var i = 0; i < 4; i++) {
      ret += '<div style="font-size: 14px; margin-top: 10px; margin-bottom: 10px; border-bottom: 1px solid rgba(80, 80, 80, 0.3); padding-bottom: 5px;\">'
         + '<h3 style="margin-bottom: 5px">' + res[i]["Nom du PDV"] +', ' + (res[i]["dist"] * 111).toFixed(1) + 'km </h3>'
      	 +res[i]["Adresse"] + ', '
         + res[i]["Ville"] + ', ' + res[i]["Code postal"] + '</br>'
      	 +  {% if request.locale.iso_code == 'en' %} "Phone: "{% else %}'Téléphone: '{% endif %} 
         + ((res[i]["Téléphone"] != void 0) ? res[i]["Téléphone"]: '' ) + '</br> </div>'
    }
    ret += "<STYLE>@media only screen and (max-width: 768px) {#wrapper {margin-bottom: 650px !important;}} #over_map > div > input {width: calc(100% - 45px) !important;} .cross{display: block !important;}</STYLE>"
    doc.innerHTML = ret;
    
  }


